<h1> Events</h1>
<p> Welcome to the Chat choice page</p>
<%if (req.user) { %>
  <form method="get" action="/chat">
  <select name="events">
  <% for(var i=0; i<eventobj.length; i++) {%>
    <% eventobj[i].events.forEach(function(event){%>
      <% for(var key in event) {%>
        <% if(event.hasOwnProperty(key)){ %>
          <% if(key==="eventname"){ %>
            <option value="<%= event[key] %>"><%= event[key] %></option>
          <% } %>
        <% } %>
      <% }%>
    <%});%>
  <% } %>
  </select>
  <br><br>
  <input type="submit" value="submit" action="/usereventjoin">
  </form>

<% } %>
